---- demand_eq  =G=  minimum requirement at demand market

demand_eq(d1)..  ship_wm(a,d1) + ship_wm(b,d1) + ship_wm(c,d1) + ship_sm(s1,d1) + ship_sm(s2,d1) =G= 75 ; (LHS = 1, INFES = 74 ****)
     
demand_eq(d2)..  ship_wm(a,d2) + ship_wm(b,d2) + ship_wm(c,d2) + ship_sm(s1,d2) + ship_sm(s2,d2) =G= 50 ; (LHS = 0, INFES = 50 ****)
     

---- balance_eq  =L=  warehouse supply demand balance

balance_eq(a)..  - ship_sw(s1,a) - ship_sw(s2,a) + ship_wm(a,d1) + ship_wm(a,d2) =L= 0 ; (LHS = -1)
     
balance_eq(b)..  - ship_sw(s1,b) - ship_sw(s2,b) + ship_wm(b,d1) + ship_wm(b,d2) =L= 0 ; (LHS = 0)
     
balance_eq(c)..  - ship_sw(s1,c) - ship_sw(s2,c) + ship_wm(c,d1) + ship_wm(c,d2) =L= 0 ; (LHS = 0)
     

---- capacity_eq  =L=  warehouse capacity

capacity_eq(a)..  - 999*build(a) + ship_wm(a,d1) + ship_wm(a,d2) =L= 0 ; (LHS = 0)
     
capacity_eq(b)..  - 60*build(b) + ship_wm(b,d1) + ship_wm(b,d2) =L= 0 ; (LHS = 0)
     
capacity_eq(c)..  - 70*build(c) + ship_wm(c,d1) + ship_wm(c,d2) =L= 0 ; (LHS = 0)
     

---- configure_eq  =L=  only one warehouse

configure_eq..  build(a) + build(b) + build(c) =L= 1 ; (LHS = 0)
     
